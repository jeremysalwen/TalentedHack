    lv2:index 0;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:symbol "midi_out";
    lv2:name "MIDI Out";
    lv2:portProperty lv2:connectionOptional;
  ] , [
    a ev:EventPort, lv2:InputPort;
    lv2:index 1;
    ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent>;
    lv2:symbol "midi_in";
    lv2:name "MIDI In";
    lv2:portProperty lv2:connectionOptional;
  ] , [
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "in";
    lv2:name "Audio In";
  ] , [
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 3;
    lv2:symbol "out";
    lv2:name "Audio Out";
  ],[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "mix";
    lv2:name   "Mix";
    lv2:minimum 0;
    lv2:maximum 1;
    lv2:default 1;
  ],[
    a lv2:ControlPort, lv2:InputPort;
